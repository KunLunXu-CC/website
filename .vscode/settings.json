{
  // -----------统一配置--------------
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  // stylelint 插件允许检查的文件
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "scss",
    "sass",
  ],
  "cSpell.words": [
    "antd",
    "Cascader",
    "codegen",
    "conds",
    "endregion",
    "jsencrypt",
    "kunlunxu",
    "nextui",
    "reduxjs",
    "shipinquanping",
    "tanstack",
    "tinify"
  ],
}
